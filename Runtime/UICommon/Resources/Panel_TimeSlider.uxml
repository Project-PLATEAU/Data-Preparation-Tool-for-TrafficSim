                    <ui:RadioButton name="SpeedOption2" text="0.5" style="max-width: 60px; min-width: 60px;"/>
                    <ui:RadioButton name="SpeedOption3" text="1.0" style="max-width: 60px; min-width: 60px;"/>
                    <ui:RadioButton name="SpeedOption4" text="2.0" style="max-width: 60px; min-width: 60px;"/>
                    <ui:RadioButton name="SpeedOption5" text="4.0" style="max-width: 60px; min-width: 60px;"/>
